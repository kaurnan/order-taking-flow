{
  "name": "moleculer-flowflex",
  "version": "1.0.0",
  "description": "My Moleculer-based microservices project",
  "scripts": {
    "dev": "moleculer-runner --env --repl --hot services/communication/**/*.service.js services/api.service.js services/ums/**/*.service.js services/utility/**/*.service.js services/integration/**/*.service.js",
    "start": "moleculer-runner",
    "cli": "moleculer connect NATS",
    "ci": "jest --watch",
    "test": "jest --coverage",
    "lint": "eslint services",
    "dc:up-oldstaging": "docker compose -f docker-compose.oldstaging.yml up --build -d",
    "dc:up": "docker compose -f docker-compose.yml up --build -d",
    "dc:logs": "docker compose -f docker-compose.yml logs -f",
    "dc:down": "docker compose -f docker-compose.yml down",
    "dc:up-local": "docker compose -f docker-compose.local.yml up --build -d",
    "dc:down-local": "docker compose -f docker-compose.local.yml down",
    "temporal:gateway": "node temporal-gateway.js",
    "temporal:worker": "node temporal-worker.js",
    "temporal:worker:cancellation": "node temporal-worker-cancellation.js",
    "temporal:worker:catalogue": "node temporal-worker-catalogue.js",
    "webhook:endpoint": "node webhook-endpoint.js",
    "test:catalogue": "node test-integration-simple.js",
    "test:api": "node test-api-endpoints.js",
    "quick-start": "node quick-start.js",
    "setup:check": "node quick-start.js"
  },
  "keywords": [
    "microservices",
    "moleculer"
  ],
  "author": "",
  "devDependencies": {
    "eslint": "^8.25.0",
    "jest": "^27.5.1",
    "jest-cli": "^27.5.1",
    "moleculer-cli": "^0.8.2",
    "moleculer-repl": "^0.7.3",
    "nats": "^2.29.3"
  },
  "dependencies": {
    "@bull-board/api": "^6.12.0",
    "@bull-board/express": "^6.12.0",
    "@google-cloud/pubsub": "^5.0.0",
    "@google-cloud/scheduler": "^5.2.0",
    "@google-cloud/storage": "^7.16.0",
    "@google-cloud/tasks": "^6.1.0",
    "@google-cloud/workflows": "^4.3.0",
    "@supabase/supabase-js": "^2.49.4",
    "@temporalio/activity": "^1.13.0",
    "@temporalio/client": "^1.13.0",
    "@temporalio/worker": "^1.13.0",
    "@temporalio/workflow": "^1.13.0",
    "archiver": "^7.0.1",
    "argon2": "^0.41.1",
    "awesome-phonenumber": "^7.4.0",
    "axios": "^0.27.2",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.2",
    "bullmq": "^5.56.7",
    "countries-list": "^3.1.1",
    "country-list": "^2.3.0",
    "csv-parser": "^3.2.0",
    "csv-writer": "^1.6.0",
    "currency-converter-lt": "^2.0.1",
    "dotenv": "^16.4.7",
    "exceljs": "^4.4.0",
    "express": "^5.1.0",
    "form-data": "^4.0.3",
    "ioredis": "^5.0.0",
    "jsonwebtoken": "^9.0.2",
    "moleculer": "^0.14.26",
    "moleculer-db": "^0.8.28",
    "moleculer-db-adapter-mongo": "^0.4.15",
    "moleculer-db-adapter-mongoose": "^0.10.0",
    "moleculer-web": "^0.10.4",
    "mongodb": "^6.16.0",
    "nodemailer": "^6.10.1",
    "pg-sdk-node": "https://phonepe.mycloudrepo.io/public/repositories/phonepe-pg-sdk-node/releases/v2/phonepe-pg-sdk-node.tgz",
    "razorpay": "^2.9.6",
    "whatsapp": "^0.0.5-Alpha",
    "workerpool": "^9.2.0"
  },
  "engines": {
    "node": ">= 16.x.x"
  },
  "jest": {
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "rootDir": "./services",
    "roots": [
      "../test"
    ]
  }
}
